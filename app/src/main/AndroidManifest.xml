<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppersue">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShopperSue">
        <activity
            android:name=".ViewItem"
            android:label="@string/title_activity_view_item"
            android:theme="@style/Theme.ShopperSue.NoActionBar"></activity>
        <activity
            android:name=".AddItem"
            android:label="@string/title_activity_add_item"
            android:theme="@style/Theme.ShopperSue.NoActionBar" />
        <activity
            android:name=".VeiwList"
            android:label="@string/title_activity_veiw_list"
            android:theme="@style/Theme.ShopperSue.NoActionBar" />
        <activity
            android:name=".CreateList"
            android:label="@string/title_activity_create_list"
            android:theme="@style/Theme.ShopperSue.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ShopperSue.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>